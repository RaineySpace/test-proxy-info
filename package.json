{
  "name": "test-proxy-info",
  "version": "1.1.0",
  "description": "代理信息测试",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "SKIP_INTEGRATION_TESTS=false vitest run __tests__/integration.test.ts",
    "test:unit": "SKIP_INTEGRATION_TESTS=true vitest run",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm run clean && pnpm run build && pnpm test && changeset publish",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm test"
  },
  "keywords": [
    "proxy",
    "test-proxy-info",
    "proxy-checker"
  ],
  "author": "RaineySpace",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaineySpace/test-proxy-info.git"
  },
  "bugs": {
    "url": "https://github.com/RaineySpace/test-proxy-info/issues"
  },
  "homepage": "https://github.com/RaineySpace/test-proxy-info#readme",
  "dependencies": {
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "eslint": "^8.56.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "~5.1.6",
    "vitest": "^2.1.9"
  }
}